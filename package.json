{
   "name": "nickl-extras",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "server": "ts-node src/index.ts",
      "start": "node ./build/src/index.js",
      "gcp-build": "tsc -p .",
      "build": "tsc --build",
      "dev": "nodemon -w *.ts -w .env src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "bundle": "ts-node bin/bundle-admin.ts",
      "tsoa_build": "tsoa spec-and-routes",
      "tsoa_build_and_run": "tsoa spec-and-routes && tsc --build && node ./build/src/index.js",
      "create-env": "printenv > .env"
   },
   "dependencies": {
      "@adminjs/bundler": "^1.1.0",
      "@adminjs/design-system": "^2.1.2",
      "@adminjs/express": "^4.0.2",
      "@adminjs/typeorm": "^2.0.1",
      "@types/node": "^17.0.18",
      "body-parser": "^1.19.1",
      "class-validator": "^0.13.2",
      "cors": "^2.8.5",
      "dotenv": "^16.0.0",
      "express": "^4.17.2",
      "express-formidable": "^1.2.0",
      "express-validator": "^6.14.0",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.3.0",
      "ts-node": "^10.4.0",
      "tsoa": "^3.14.1",
      "typeorm": "0.2.43",
      "typescript": "^4.5.2"
   },
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/node": "^16.11.10",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/uuid": "^8.3.4",
      "nodemon": "^2.0.15"
   }
}
